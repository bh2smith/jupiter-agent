{
  "name": "solana",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "agent": "concurrently \"tsx src/index.ts\" \"make-agent dev\"",
    "api": "tsx src/index.ts",
    "build": "tsc",
    "deploy": "make-agent deploy -u https://jupiter-agent.vercel.app/",
    "lint": "biome check && prettier --check .",
    "fmt": "biome format --write . && prettier --write .",
    "sample": "bun run tinker/sample.ts",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "@solana-program/token": "^0.7.0",
    "@solana/addresses": "^4.0.0",
    "@solana/rpc": "^4.0.0",
    "@solana/web3.js": "^1.98.4",
    "bs58": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jup-fork": "^6.0.46",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/swagger-ui-react": "^5.18.0",
    "biome": "^0.3.3",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "make-agent": "^0.4.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  }
}
